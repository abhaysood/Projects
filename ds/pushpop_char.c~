#include<stdio.h>

#define MAX 20
char val;
int top;
//---------
void push(char stack[],char val)
{
 if(top==MAX-1)
 {
 printf("overflow!!!!!!!\n");
 }
 else
 {
 ++top;
 stack[top]=val;
 }
}
//---------------


//----------------
void display(char stack[],int top)
{
 int count;
 if(top==-1)
 {
 printf("Stack is EMPTY!!!!!!!\n");
 }
 else
 {
 for(count=top;count>=0;count--)
 {
 printf("%c",stack[count]);
 }
 printf("\n");
 }
}



